{
  "name": "yawp-cli",
  "version": "1.0.21",
  "description": "YAWP! Framework JS client",
  "main": "lib/yawp.min.js",
  "scripts": {
    "build": "npm run build-node; npm run build-web",
    "build-node": "BABEL_ENV=node babel src --ignore web --source-maps --out-dir lib/node",
    "build-web": "webpack --mode=build",
    "dev-node": "BABEL_ENV=node babel src --ignore web --source-maps --watch --out-dir lib/node",
    "dev-web": "webpack --progress --colors --watch --mode=dev",
    "test": "BABEL_ENV=node mocha --compilers js:babel-core/register --colors -w ./test/*.spec.js",
    "prepublish": "npm run build",
    "analyzer": "webpack --mode=build --json | webpack-bundle-size-analyzer"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.10.1",
    "babel-core": "^6.10.4",
    "babel-eslint": "^6.1.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-syntax-async-functions": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-node5": "^11.1.0",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "eslint": "^2.13.1",
    "eslint-loader": "^1.3.0",
    "json-loader": "^0.5.4",
    "mocha": "^2.5.3",
    "webpack": "^1.13.1",
    "webpack-bundle-size-analyzer": "^2.0.2",
    "yargs": "^4.7.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/feroult/yawp.git"
  },
  "keywords": [
    "yawp",
    "client",
    "api",
    "rest",
    "appengine"
  ],
  "author": "Fernando Ultremare",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/feroult/yawp/issues"
  },
  "homepage": "http://yawp.io",
  "dependencies": {
    "node-fetch": "^1.5.3"
  }
}
